<service>
  <id>nginx</id>
  <name>Nginx Web Server</name>
  <description>Nginx web server (managed by Scoop)</description>
  <executable>D:\\SoftwareApps\\Scoop\\apps\\nginx\\current\\nginx.exe</executable>
  <logpath>D:\\SoftwareApps\\Scoop\\apps\\nginx\\current\\logs</logpath>
  <logmode>roll</logmode>
  <depend></depend>
  <startargument>-p</startargument>
  <startargument>D:\\SoftwareApps\\Scoop\\apps\\nginx\\current</startargument>
  <stopexecutable>D:\\SoftwareApps\\Scoop\\apps\\nginx\\current\\nginx.exe</stopexecutable>
  <stopargument>-p</stopargument>
  <stopargument>D:\\SoftwareApps\\Scoop\\apps\\nginx\\current</stopargument>
  <stopargument>-s</stopargument>
  <stopargument>stop</stopargument>
</service>
