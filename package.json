{
  "name": "use",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "mac:init": "sh ./bash/mac-init.sh",
    "mac:backup": "brew bundle dump --no-vscode --force --file=./mac/Brewfile",
    "mac:setup": "brew bundle install --file=./mac/Brewfile",
    "mac:sync": "sh ./bash/mac-zsh-sync.sh",
    "win:init": "bash.exe ./bash/win-init.sh",
    "win:backup": "scoop export > ./windows/scoop_backup.json",
    "win:setup": "scoop import ./windows/scoop_backup.json",
    "win:sync": "bash.exe ./bash/win-zsh-sync.sh",
    "win:zsh": "bash.exe ./bash/win-zsh-install.sh",
    "win:clink": "bash.exe ./bash/win-clink-setup.sh",
    "win:git-extras": "bash.exe ./bash/win-git-extras-install.sh",
    "other:sync": "bash.exe ./bash/other-sync.sh"
  },
